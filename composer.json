{
    "name": "dud/contao-bestellsystem-bundle",
    "type": "contao-bundle",
    "description": "Bestellsystem-Erweiterung für Contao Open Source CMS",
    "homepage": "https://www.drum-und-dran",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gunnar Lorenz – Haeuschkel und Lorenz GbR, Drum und Dran",
            "homepage": "https://www.drum-und-dran.de"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.8.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Dud\\ContaoBestellsystemBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Dud\\ContaoBestellsystemBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dud\\ContaoBestellsystemBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://www.drum-und-dran.de",
        "source": "https://www.drum-und-dran.de"
    }
}
